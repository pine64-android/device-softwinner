on init
    insmod /nand.ko
    insmod /sw-device.ko debug_mask=0xff ctp_mask=1
    insmod /sunxi-keyboard.ko
    insmod /disp.ko
    insmod /lcd.ko

    mkdir /extsd
    mkdir /bootloader
    exec /sbin/busybox sh /sbin/init_parttion.sh

service console /sbin/sh
    console
    disabled
    user shell
    group log
